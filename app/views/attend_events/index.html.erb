<h1>Listing Attend Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attend_events.each do |attend_event| %>
      <tr>
        <td><%= link_to 'Show', attend_event %></td>
        <td><%= link_to 'Edit', edit_attend_event_path(attend_event) %></td>
        <td><%= link_to 'Destroy', attend_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attend event', new_attend_event_path %>
